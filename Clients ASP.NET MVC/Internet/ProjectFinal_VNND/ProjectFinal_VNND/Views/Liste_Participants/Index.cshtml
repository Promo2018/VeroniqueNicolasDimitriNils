@model IEnumerable<ProjectFinal_VNND.Models.Liste_Participants>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Dossiers.id_dossier)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Personnes.id_personne)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Personnes.prenom)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Dossiers.id_dossier)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Personnes.id_personne)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Personnes.prenom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Personnes.nom)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id_listparticipant }) |
                @Html.ActionLink("Details", "Details", new { id = item.id_listparticipant }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.id_listparticipant })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
