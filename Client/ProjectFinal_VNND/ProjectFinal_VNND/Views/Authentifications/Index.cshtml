@model IEnumerable<ProjectFinal_VNND.Models.Authentifications>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Liste des Comptes utilisateurs</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Ajouter")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.mot_de_passe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Statuts.statut)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.mot_de_passe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Statuts.statut)
            </td>
            <td>
                @Html.ActionLink("Edit", "Modifier", new { id=item.id_auth }) |
                @Html.ActionLink("Details", "Informations", new { id=item.id_auth }) |
                @Html.ActionLink("Delete", "Supprimer", new { id=item.id_auth })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
